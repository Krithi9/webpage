	<script type="text/javascript">
		class students
		{
			constructor(Name,Reg,Stream,Age,Gender)
			{
				this.Name = Name;
				this.Reg = Reg;
				this.Stream = Stream;
				this.Age = Age;
				this.Gender = Gender;
			}
		}

		const std1 = new students ("Krithi",91,"BCA",20,"Female")
